#!/bin/bash
# Mock Claude CLI for testing
# Returns a successful stream-json response

# Emit system init event
echo '{"type":"system","subtype":"init","session_id":"test-session-001","model":"sonnet"}'
# Emit assistant text response
echo '{"type":"assistant","message":{"content":[{"type":"text","text":"Task completed successfully. I made the requested changes."}]}}'
# Emit final result event
echo '{"type":"result","subtype":"success","session_id":"test-session-001","duration_ms":1000,"num_turns":1,"total_cost_usd":0.01}'
